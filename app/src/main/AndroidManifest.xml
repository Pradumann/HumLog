<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.praduman.humlog" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".LogInActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".SignUpChoiceActivity"
            android:label="@string/title_activity_sign_up_choice"
            android:parentActivityName=".LogInActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.praduman.humlog.LogInActivity" />
        </activity>
        <activity
            android:name=".CustomerSignUpPageOneActivity"
            android:label="@string/title_activity_customer_sign_up_page_one"
            android:parentActivityName=".SignUpChoiceActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.praduman.humlog.SignUpChoiceActivity" />
        </activity>
        <activity
            android:name=".CustomerSignUpPageTwoActivity"
            android:label="@string/title_activity_customer_sign_up_page_two"
            android:parentActivityName=".CustomerSignUpPageOneActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.praduman.humlog.CustomerSignUpPageOneActivity" />
        </activity>
        <activity
            android:name=".TradesmanSignUpPageOneActivity"
            android:label="@string/title_activity_tradesman_sign_up_page_one"
            android:parentActivityName=".SignUpChoiceActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.praduman.humlog.SignUpChoiceActivity" />
        </activity>
        <activity
            android:name=".TradesmanSignUpPageTwoActivity"
            android:label="@string/title_activity_tradesman_sign_up_page_two"
            android:parentActivityName=".TradesmanSignUpPageOneActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.praduman.humlog.TradesmanSignUpPageOneActivity" />
        </activity>
        <activity
            android:name=".HomeActivity"
            android:label="@string/title_activity_home" >
        </activity>
        <activity
            android:name=".LauncherActivity"
            android:label="@string/title_activity_launcher" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PostAdActivity"
            android:label="@string/title_activity_post_ad" >
        </activity>
        <activity
            android:name=".EditProfileActivity"
            android:label="@string/title_activity_edit_profile"
            android:parentActivityName=".HomeActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.praduman.humlog.HomeActivity" />
        </activity>
        <activity
            android:name=".MyAdActivity"
            android:label="@string/title_activity_my_ad"
            android:parentActivityName=".HomeActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.praduman.humlog.HomeActivity" />
        </activity>
    </application>

</manifest>
